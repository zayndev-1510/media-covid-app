<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.admin.PesanPage"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <LinearLayout
        android:orientation="vertical"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:gravity="center_vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:ignore="UselessLeaf">
        <ImageView
            android:layout_width="250dp"
            android:src="@drawable/emot_ok_v2"
            android:layout_gravity="center"
            android:layout_height="250dp"
            tools:ignore="ContentDescription" />
        <TextView
            android:text="Hallo,Admin Videonya berhasil bisa dicek halaman data video dengan mengklik klik tombol dibawah ini"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_medium"
            android:padding="10dp"
            android:textSize="15sp"
            android:layout_margin="10dp"
            tools:ignore="HardcodedText" />
        <TextView
            android:id="@+id/data_video"
            android:text="Data video"
            android:padding="10dp"
            android:fontFamily="@font/poppins_medium"
            android:textSize="20sp"
            android:textColor="@color/white"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:textAlignment="center"
            android:background="@color/btn_color"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:ignore="HardcodedText" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>